            <description>Source of document.</description>
            <supertypeName>uima.tcas.Annotation</supertypeName>
            <features>
                <featureDescription>
                    <name>value</name>
                    <description>Actual source type value.</description>
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                </featureDescription>
            </features>
        </typeDescription>  
        <typeDescription>
            <name>org.apache.uima.textpresso.filename</name>
